    "@nuxthub/core": "^0.10.1",
    "@pinia/colada": "^0.18.1",
    "@pinia/colada-nuxt": "^0.2.4",
    "@pinia/nuxt": "^0.11.3",
    "@upstash/redis": "^1.35.7",
